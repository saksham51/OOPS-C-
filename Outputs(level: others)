                     //Lecture 3 Conditionals and loops
A)#include <iostream>
using namespace std;
int main()
{
    int x = 5; 
    if (x < 6)
        cout <<  “Hello ”;
    if(x == 5){
        cout << “Hi ”;
    }
    else { 
        cout << “Hey ”;
    } 
}
ANSWER:Hello Hi

B)#include <iostream>
using namespace std;
int main() {
    int x = 15;
    if(x <= 15) {
        cout << "Inside if ";
    } else if(x == 15) {
        cout << "Inside else if ";
    }
    cout << x;
}
ANSWER:Inside if 15

C)#include <iostream>
using namespace std; 
int main()
{
        int var1 = 5; 
        int var2 = 6;
        if ((var2 = 1) == var1)
           cout << var2;
        else 
            cout  << (var2 + 1); 
}
ANSWER:2

D)For what values of the x is an if-statement block not executed ?
       if((1+x) != (1-x)) {
          cout<<"In if block";
         }
ANSWER:0

E)#include <iostream>
using namespace std;
int main() {
    int a = 10, b = 20, c = 100;
    if(a <= b || c <= b) {
        cout << "hello" << endl;
    }
    else if(a <= b || a <= c) {
        cout << "hi" << endl;
    }
    else {
        cout << "hey" << endl;
    }
}
ANSWER:hello

F)#include <iostream>
using namespace std;
int main() {
    int a = 10, b = 20, c = 30;
    if(a <= b && !b) {
        cout << "hello";
    }
    else if(c >= a && c >= b) {
        cout << "hi";
    }
    else {
        cout << "hey";
    }
}
ANSWER:hi

                   //Lecture 6 Operators and for loops
A)#include <iostream>
using namespace std;
int main() 
{    
         int x, y = 1;
         x = 10;
         if (x != 10 && x / 0 == 0)
            cout << y;
         else
             cout << ++y;
}
ANSWER:2

B)#include <iostream>
using namespace std;
int main()
{   
    int x = 15;
    int y = x++;
    int z = ++x;
    cout << y << “ “ << z;
}
ANSWER:15 17

C)#include <iostream>
using namespace std;
int main() 
{        
         int g = 3;
         cout << (++g * 8);
}
ANSWER:32

D)#include <iostream>
using namespace std;
int main()
{
    int x = 10;
    int y = 20;
    if(x++ > 10 && ++y > 20 ){
    cout << "Inside if ";
    } else{
    cout << "Inside else ";
    }
    cout << x << “ “ << y;
}
ANSWER:Inside else 11 20

E)#include <iostream>
using namespace std;
int main()
{
    int x = 10;
    int y = 20;
    if(x++ > 10 || ++y > 20 ){
    cout << "Inside if ";
    } else{
    cout << "Inside else ";
    }
    cout << x << “ “ << y;
}
ANSWER:Inside else 11 21

F)What will be the output the following code ?
for(int i = 0; i < 5; i = i + 1){
    cout << i << " ";
    i = i + 1;
}
ANSWER:0 2 4

G)What will be the output the following code?
for(int i = 1; i < 5; i = i + 1){
    cout << i << " ";
    i = i - 1;
}
ANSWER:Infinite 1s

H)for(int i = 0; i < 2; i = i + 1) {
     for(int j = 0; j < 2; j = j + 1) {
          if (j == 1)
              break;
          cout << j << " ";
      }
 }
 ANSWER:0 0
 
 I)for(int i = 0; i < 5; i = i + 1) {
     if(i == 2) 
           continue;
    cout << i << " ";
 }
 ANSWER:0 1 3 4
 
 J)Which of these jump statements can skip processing remainder of code in its body for a particular iteration ?
 ANSWER:continue
 
 K)int i = 1;
while(i < 5) {
    if(i == 3) {
        break;
    }
    cout << i << " ";
    i++;
}
ANSWER:1 2

L)int i = 1;
while(i < 5) {
    if(i == 3) {
        continue;
    }
    cout << i << " ";
    i++;
}

M)int i = 1;
while(i < 5) {
    if(i == 3) {
        continue;
    }
    cout << i << " ";
    i++;
}
ANSWER:1 2 Infinite Loop

N)int i = 1;
while(i < 3) {
    int j = 1;
    while(j < 5) {
        if(j == 3) {
            break;
        }
        cout << j << " ";
        j++;
    }
    i++;
}
ANSWER:1 2 1 2

P)int i = 1;
while(i < 3) {
    int j = 0;
    while(j < 5) {
        j++;
        if(j == 3) {
            continue;
        }
        cout << j << " ";
    }
    i++;
}
ANSWER:1 2 4 5 1 2 4 5

Q)#include <iostream>
using namespace std;
int main() {
    int a = 10;
    if(a > 5) {
        int b = 10;
    }
    cout << b << endl;
}
ANSWER:Yes

R)#include <iostream>
using namespace std;
int main() {
    int a = 10;
    if(a > 5) {
        int a = 100;
    }
    cout << a << endl;
}
ANSWER:No

S)What is the output ?
#include <iostream>
using namespace std;
int main() {
    int a = 10;
    if(a > 5) {
        int a = 100;
    }
    else {
        int a = 110;
    }
    cout << a << endl;
}
ANSWER:10

T)Will following code generate error ?
#include <iostream>
using namespace std;
int main() {
    for(int i = 0; i < 3; i++) {
        cout << i << " ";
    }
    cout << i << " ";
}
ANSWER:Yes

U)#include <iostream>
using namespace std; 
int main() {
    int a = 10;
    while(a > 5) {
        int a = 1;
        cout << a << " ";
        a--;
    }
}
ANSWER:Infinite Loop
